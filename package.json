{
  "name": "jaysn",
  "version": "0.1.0",
  "description": "A databaseless data storing with one JSON file.",
  "homepage": "https://lowsprofile.github.io/jaysn/",
  "repository": "lowsprofile/jaysn",
  "author": "Eries Trisnadi <me@eries.id> (https://eries.id)",
  "license": "MIT",
  "keywords": [],
  "main": "dist/jaysn.js",
  "module": "dist/jaysn.es.js",
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb-base"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "immutable": "^3.8.2",
    "path": "^0.12.7",
    "stemmer": "^1.0.2",
    "superstruct": "^0.4.5",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-function-name": "^6.24.1",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.12.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.3.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-strip-banner": "^0.2.0",
    "rollup-plugin-uglify": "^2.0.1",
    "sinon": "^4.1.2",
    "uglify-es": "^3.2.0",
    "uglify-save-license": "^0.4.1"
  },
  "scripts": {
    "lint": "eslint src test tools",
    "test": "mocha --require babel-register",
    "clean:dist": "rimraf dist",
    "test:watch": "mocha --require babel-register --watch",
    "test:cover": "nyc --require babel-register mocha --timeout=3000",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "run-s clean:dist bundle",
    "bundle": "run-s bundle:*",
    "bundle:cjs": "rollup -c tools/build-cjs.js",
    "bundle:es": "rollup -c tools/build-es.js",
    "prepublish": "run-s build",
    "publish:docs": "easystatic deploy docs --repo lowsprofile/jaysn",
    "start": "easystatic start docs"
  }
}
